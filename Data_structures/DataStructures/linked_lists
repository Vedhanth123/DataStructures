// data and link must be grouped
#include <stdlib.h>
#include <stdio.h>
struct node
{
    int data;          // it stores data
    struct node *link; // it must point to address of next node
};

typedef struct node node; // it gives user name to struct node
node *first, *last, *newnode;

void insert()
{
    int data;
    newnode = (node *)malloc(sizeof(node)); // create the node
    printf("enter the data");
    scanf("%d", &data);

    newnode->data = data; // inserting data

    newnode->link = NULL;
    if (first == NULL) //no node exist in memory
    {
        // printf("no node in memory,it creates node");
        first = newnode;
        last = newnode;
    }

    else
    {

        last->link = newnode; // link the nodes
        last = newnode;       // to point to last node
    }
}

void display()
{
    node *temp = NULL;

    temp = first; // temp points to first
    // printf("list of nodes \n");
    while (temp != NULL)
    {
        printf("\n%d", temp->data);
        temp = temp->link; // it will move to next node
    }
}

void main()
{
    int n, i;
    printf("enter number nodes");
    scanf("%d", &n);
    for (i = 1; i <= n; i++)
        insert(); // 10 nodes

    display();
}
